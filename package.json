{
  "name": "scrapeobot-server",
  "version": "1.0.0",
  "description": "Scrapeobot Server - We already told you that dum dum",
  "main": "server.js",
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/server",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon"
  },
  "keywords": [
    "scrapes",
    "floors",
    "pumps",
    "dumps"
  ],
  "author": "Ambuscade",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@debut/indicators": "^1.3.19",
    "axios": "^0.21.4",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "kucoin-node-sdk": "^1.1.1",
    "lightweight-charts": "^4.1.1",
    "mongodb": "^4.11.0",
    "mongoose": "^6.6.5",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12"
  }
}
